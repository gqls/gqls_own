{
  "name": "frontend_webpack_karma_jasmine_es6",
  "version": "0.0.1",
  "description": "Webpack babel es5 and karma & jasmine for testing",
  "repository": "http://example.com",
  "license": "GPL-3.0",
  "scripts": {
    "build": "npm install --no-bin-links && npm run build:dev",
    "build:dev": "node webpack -g --no-bin-links --display-error-details src/components/root/root.js dist/js/root.js --config webpack.config.dev.js",
    "clean": "npm run rimraf dist/js/*",
    "lint": "eslint src",
    "test": "karma start --single-run --no-auto-watch karma.config.js"
  },
  "keywords": [
    "es6",
    "babel",
    "webpack",
    "jasmine",
    "karma",
    "istanbul",
    "boilerplate"
  ],
  "dependencies": {},
  "devDependencies": {
    "babel-core": "^6.4.0",
    "babel-eslint": "^7.2.3",
    "babel-istanbul": "^0.12.2",
    "babel-loader": "^7.1.0",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-polyfill": "^6.23.0",
    "babel-preset-es2015": "^6.3.13",
    "babel-preset-react": "^6.24.1",
    "babel-runtime": "^6.23.8",
    "enhanced-resolve": "^3.1.0",
    "eslint": "^4.0.0",
    "eslint-config-airbnb": "^15.0.1",
    "eslint-loader": "^1.6.3",
    "glob": "^7.1.2",
    "istanbul-instrumenter-loader": "^2.0.0",
    "karma": "^1.7.0",
    "loader-utils": "^1.1.0",
    "minimatch": "^3.0",
    "node-pre-gyp": "^0.6.36",
    "react": "^15.6.1",
    "react-devtools": "^2.3.3",
    "react-dom": "^15.6.1",
    "react-redux": "^5.0.5",
    "react-router": "^4.1.1",
    "redux": "^3.7.1",
    "rimraf": "^2.6.1",
    "tapable": "^0.2.6",
    "watchpack": "^1.3.1",
    "webpack": "^3.0.0",
    "webpack-dev-server": "^2.5.0"
  }
}
